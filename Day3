#include <bits/stdc++.h>
#include<math.h>

using namespace std;



int main()
{
    int n;
    cin >> n;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');
    
    if((n%2 == 0) && ((1 < n) && (n < 6)) )
      {cout<<"Not Weird";
      }
    else if((n%2 == 0) && ((5 < n) && (n< 21)) )
      {cout<<"Weird";
      } 
    else if((n%2 ==0) && (n > 20))
      {cout<<"Not Weird";
      }  
    else if(n%2 != 0)
      {cout<<"Weird";
      } 
    return 0;
}
